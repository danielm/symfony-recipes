{
    "manifests": {
        "danielm/symfony-demo-bundle": {
            "manifest": {
                "bundles": {
                    "Danielm\\DemoBundle\\DanielmDemoBundle": [
                        "all"
                    ]
                },
                "copy-from-package": {
                    ".recipe/package.yaml": "%CONF_DIR%/packages/danielm_demo.yaml",
                    ".recipe/routes.yaml": "%CONF_DIR%/routes/danielm_demo.yaml"
                },
                "env": {
                    "DEMO_SAMPLE_ENV": "Some Random String"
                },
                "post-install-output": [
                    "  * It works!:",
                    "",
                    "  * The Demo Flex Recipe and bundle have been installed! ",
                    "",
                    "  * <fg=blue>Read</> the documentation at <comment>https://github.com/danielm/symfony-recipes/</>"
                ]
            },
            "ref": "260a3aa1ef41e869c67ab10ffef11b82e753db38"
        }
    }
}
