{
    "manifests": {
        "danielm/symfony-demo-bundle": {
            "manifest": {
                "bundles": {
                    "Danielm\\DemoBundle\\DanielmDemoBundle": [
                        "all"
                    ]
                }
            },
            "copy-from-recipe": {
                "config/": "%CONFIG_DIR%/"
            },
            "post-install-output": [
                "  * It works!:",
                "",
                "  * The Demo Flex Recipe and bundle have been installed! ",
                "",
                "  * <fg=blue>Read</> the documentation at <comment>https://github.com/danielm/symfony-recipes/</>"
            ],
            "env": {
                "DEMO_SAMPLE_ENV": "Some Random String"
            },
            "files": {
                "config/packages/danielm_demo.yaml": {
                    "contents": [
                        "danielm_demo:",
                        "  some_node: true",
                        "    required_number_value: 12",
                        "    optional_value: 'uwu'",
                        "    env_value: '%env(DEMO_SAMPLE_ENV)%",
                        "    #secret_value: '%DEMO_SAMPLE_SECRET%'",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/danielm_demo.yaml": {
                    "contents": [
                        "danielm_demo_bundle:",
                        "  resource: \"@DanielmDemoBundle/config/routes.yaml\"",
                        "  prefix: \"/demo\"",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "260a3aa1ef41e869c67ab10ffef11b82e753db38"
        }
    }
}
